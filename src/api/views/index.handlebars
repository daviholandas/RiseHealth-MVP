<div id="testeIO"></div>

<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io();
  socket.on('connect',function(){
    if(socket.connected){
      socket.on('msg', data =>{
        document.getElementById('testeIO').innerHTML = data
      })
    }else{
      document.getElementById('testeIOD').innerHTML =  'Desconectado!'
    }
  })
</script>

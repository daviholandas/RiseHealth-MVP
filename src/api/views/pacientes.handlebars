<div class="container-fluid mt-5 ">
    <table class="table-responsive-lg table-bordered tabela">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Nome</th>
                <th scope="col">Data de Nascimento</th>
                <th scope="col">Sexo</th>
                <th scope="col" colspan="2" >Documentos</th>
                <th scope="col">Endereço</th>
                <th scope="col">Telefone</th>
                <th scope="col">E-mail</th>
                <th scope="col">Informações</th>
                <th scope="col">Tipo</th>
            </tr>
        </thead>
        <tbody>
            {{#pacientes}}
            <tr>
                <th scope="row">{{_id}}</th>
                <td>{{nome}}</td>
                <td>{{nascimento}}</td>
                <td>{{sexo}}</td>
                <td>{{documentos.cpf}}</td>
                <td>{{documentos.rg}}</td>
                <td></td>
                <td>{{telefone}}</td>
                <td>{{email}}</td>
                <td>{{informacoes}}</td>
                <td>{{__type}}</td>
            </tr>
            {{/pacientes}}
        </tbody>
    </table>
</div>